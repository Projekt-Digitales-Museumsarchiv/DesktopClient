<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.canvas.Canvas?>
<GridPane alignment="CENTER" xmlns:fx="http://javafx.com/fxml"
          fx:controller="de.sfnbg.archivdesktopclient.DesktopController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="imageTitle" GridPane.rowIndex="1" GridPane.columnIndex="1" GridPane.rowSpan="1" GridPane.columnSpan="3" text="Gelesenes Bild"/>
    <Canvas height="327.0" width="312.0" GridPane.columnIndex="1" GridPane.rowIndex="2" GridPane.rowSpan="3" GridPane.columnSpan="3"/>
    <Button GridPane.columnIndex="1" GridPane.rowIndex="5" onAction="#onbnWebCamClick" text="Aufnehmen"/>
    <Button GridPane.columnIndex="2" GridPane.rowIndex="5" onAction="#onbnScannerClick" text="Scannen"/>
    <Button GridPane.columnIndex="3" GridPane.rowIndex="5" onAction="#onbnUploadClick" text="Datei"/>


</GridPane>
